<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="303.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vidmot.LagController">
   <children>
      <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" layoutX="102.0" layoutY="46.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="songName" alignment="CENTER" layoutX="-2.0" layoutY="196.0" prefHeight="27.0" prefWidth="404.0" text="Label" />
      <ProgressBar fx:id="songProgress" layoutX="102.0" layoutY="227.0" prefHeight="18.0" prefWidth="208.0" progress="0.0" />
      <Button fx:id="playButtong" layoutX="180.0" layoutY="245.0" mnemonicParsing="false" onAction="#playPause" prefHeight="42.0" prefWidth="52.0" text="pause" />
      <Button fx:id="homeButton" layoutX="76.0" layoutY="1.0" mnemonicParsing="false" onAction="#switchToHome" text="Home" />
      <Button fx:id="playlistButton" layoutX="284.0" layoutY="1.0" mnemonicParsing="false" text="Button" />
      <Button fx:id="nextButtton" layoutX="258.0" layoutY="253.0" mnemonicParsing="false" onAction="#nextSong" text="next" />
      <Button fx:id="prevButton" layoutX="102.0" layoutY="253.0" mnemonicParsing="false" onAction="#prevSong" text="back" />
   </children>
</AnchorPane>
